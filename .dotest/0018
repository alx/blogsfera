+<div id="navegacion">
+
+<ul>
+			<?php 	/* Widgetized sidebar, if you have the plugin installed. */
+					if ( !function_exists('dynamic_sidebar') || !dynamic_sidebar(1) ) : ?>
+			<li>
+				<?php include (TEMPLATEPATH . '/searchform.php'); ?>
+			</li>
+
