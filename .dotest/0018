+margin-left: 22px;}
+
+p.post-info {
+padding-top: 0;
+color: #666;}
+
+p.post-info img{
+    height:16px;
+    width:16px;
+    vertical-align: middle;
+}
+
+#contenido h2 a {
+color: #069;}
+
+
+
+/* =navegacion */
+
+div#navegacion {
+float:left;
+width:175px;
